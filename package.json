{
	"name": "@contexted/utils",
	"version": "0.3.1",
	"description": "Utilities for Contexted framework",
	"keywords": [
		"typescript",
		"javascript",
		"contexted",
		"event",
		"emitter",
		"route",
		"strategy"
	],
	"engines": {
		"node": ">=0.10.0"
	},
	"license": "MIT",
	"author": "https://github.com/contexted-js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/contexted-js/utils.git"
	},
	"type": "module",
	"exports": {
		"./strategy": {
			"import": "./dist/strategy/index.mjs",
			"require": "./dist/strategy/index.cjs"
		},
		"./emitter": {
			"import": "./dist/emitter/index.mjs",
			"require": "./dist/emitter/index.cjs"
		}
	},
	"scripts": {
		"build": "rm -f -- *.d.ts && rimraf dist && rollup --config rollup.config.ts --configPlugin typescript && tsc && mv ./dist/*.d.ts ./",
		"test": "jest"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/preset-env": "^7.16.5",
		"@babel/preset-typescript": "^7.16.5",
		"@contexted/core": "^2.1.1",
		"@rollup/plugin-typescript": "^8.3.0",
		"@types/jest": "^27.0.3",
		"babel-jest": "^27.4.5",
		"jest": "^27.4.5",
		"np": "^7.6.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.61.1",
		"rollup-plugin-license": "^2.6.0",
		"rollup-plugin-terser": "^7.0.2",
		"terser": "^5.10.0",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	}
}
